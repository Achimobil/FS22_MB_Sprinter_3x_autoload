<?xml version="1.0" encoding="utf-8" standalone="no"?>
<vehicle type="PalletAutoLoaderCar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://validation.gdn.giants-software.com/xml/fs22/vehicle.xsd">
	<annotation>Farming-Freunde</annotation>

    <parentFile xmlFilename="sprinter.xml">
        <attributes>
            <set path="vehicle.storeData.name" value="MB eSprinter AL"/>
            <set path="vehicle.base.sounds#filename" value="$data/vehicles/jungheinrich/efgS50S/sounds/efgS50S.xml"/>
            <set path="vehicle.base.sounds#volumeFactor" value="2.5"/>

            <set path="vehicle.motorized#forceSpeedHudDisplay" value="true"/>
            <set path="vehicle.motorized.motorStartDuration" value="500"/>
            <remove path="vehicle.motorized.motorConfigurations.motorConfiguration(3)"/>
            <remove path="vehicle.motorized.motorConfigurations.motorConfiguration(2)"/>
            <remove path="vehicle.motorized.motorConfigurations.motorConfiguration(1)"/>
            <remove path="vehicle.motorized.motorConfigurations.motorConfiguration(0)"/>
            <!-- <set path="vehicle.motorized.motorConfigurations.motorConfiguration(0)#isSelectable" value="false"/> -->
            <!-- <set path="vehicle.motorized.motorConfigurations.motorConfiguration(1)#isSelectable" value="false"/> -->
            <!-- <set path="vehicle.motorized.motorConfigurations.motorConfiguration(2)#isSelectable" value="false"/> -->
            <!-- <set path="vehicle.motorized.motorConfigurations.motorConfiguration(3)#isSelectable" value="false"/> -->
            <set path="vehicle.motorized.motorConfigurations.motorConfiguration(1)#isSelectable" value="true"/>
            <set path="vehicle.motorized.motorConfigurations.motorConfiguration(0)#isSelectable" value="true"/>
         </attributes>
    </parentFile>

    <!-- ONLY FOR ICON GENERATION -->
    <storeData>
        <name>MB Sprinter AL</name>
        <image>store_Sprinter.dds</image>
        <brand>MERCEDES</brand>
        <category>cars</category>
    </storeData>	
</vehicle>
